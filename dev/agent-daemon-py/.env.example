# ===========================================
# OUTWARD ORBIT - BROWSER AUTOMATION CONFIG
# ===========================================

# ============ API KEYS ============
# Add your API keys here (leave empty if not using)
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
GOOGLE_API_KEY=your_google_api_key_here

# ============ AI MODEL CONFIGURATION ============

# OpenAI GPT-5 Models (Released 2025)
OPENAI_MODEL=gpt-5-2025-08-07
OPENAI_MODEL_FAST=gpt-5-mini-2025-08-07
OPENAI_MODEL_MINI=gpt-5-nano-2025-08-07
OPENAI_MODEL_REASONING=gpt-5-2025-08-07

# Anthropic Claude Sonnet 4 Models (Released 2025)
ANTHROPIC_MODEL=claude-sonnet-4
ANTHROPIC_MODEL_SONNET=claude-sonnet-4
ANTHROPIC_MODEL_HAIKU=claude-haiku-4

# Google Latest Gemini Models
GOOGLE_MODEL=gemini-2.0-flash-exp
GOOGLE_MODEL_PRO=gemini-2.0-pro
GOOGLE_MODEL_ULTRA=gemini-ultra-2.0

# ============ BROWSER AGENT CONFIGURATION ============

# Server Settings
AGENT_HOST=127.0.0.1
AGENT_PORT=4823

# Browser Settings
HEADLESS=false
BLOCK_IMAGES=true
BLOCK_ADS=true
STEALTH_MODE=true
USER_DATA_DIR=./chrome-profile

# Performance Settings
DEFAULT_TIMEOUT=8000
NAVIGATION_TIMEOUT=15000
WATCHDOG_TIMEOUT=12000
KEEP_OPEN_SECONDS=20

# Search Engine Preferences
PRIMARY_SEARCH_ENGINE=google
FALLBACK_SEARCH_ENGINE=duckduckgo
ENABLE_SEARCH_FALLBACK=true

# ============ ADVANCED FEATURES ============

# Screenshot Settings
CAPTURE_SCREENSHOTS=true
SCREENSHOT_QUALITY=medium
AUTO_SAVE_SCREENSHOTS=true

# Session Management
MAX_CONCURRENT_SESSIONS=5
SESSION_TIMEOUT=300

# Debug Settings
DEBUG_MODE=false
VERBOSE_LOGGING=false
LOG_BROWSER_CONSOLE=false

# ============ EXPERIMENTAL FEATURES ============

# AI-Enhanced Browsing (requires API keys)
ENABLE_AI_ANALYSIS=false
AI_SUMMARIZE_PAGES=false
AI_EXTRACT_DATA=false

# Browser Automation Features
AUTO_SCROLL=true
SMART_WAITING=true
ELEMENT_HIGHLIGHTING=false

# ============ SECURITY SETTINGS ============

# Stealth Browsing
DISABLE_WEBDRIVER_DETECTION=true
RANDOMIZE_USER_AGENT=true
SIMULATE_HUMAN_BEHAVIOR=true

# Privacy Settings
DISABLE_COOKIES=false
DISABLE_TRACKING=true
CLEAR_DATA_ON_EXIT=false